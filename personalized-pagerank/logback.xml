<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="Console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date{"HH:mm:ss,SSSZ", UTC} %p %C{1} - %m%n</pattern>
        </encoder>
    </appender>
    <root level="error">
        <appender-ref ref="Console"/>
    </root>
</configuration>